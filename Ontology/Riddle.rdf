    <!-- http://www.semanticweb.org/mahdijafarkhani/ontologies/riddle-ontology#Color -->

    <owl:Class rdf:about="http://www.semanticweb.org/mahdijafarkhani/ontologies/riddle-ontology#Color">
        <owl:equivalentClass>
