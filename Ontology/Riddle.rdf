                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/mahdijafarkhani/ontologies/riddle-ontology#blue"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/mahdijafarkhani/ontologies/riddle-ontology#green"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/mahdijafarkhani/ontologies/riddle-ontology#red"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/mahdijafarkhani/ontologies/riddle-ontology#yellow"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty>
